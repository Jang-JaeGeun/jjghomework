<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<form action="/users/create" method="POST">
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <p>ID:</br><input type="text" name="username" placeholder="username"></p>
    <p>이메일:</br><input type="text" name="email" placeholder="email"></p>
    <p>비밀번호:</br><input type="password" name="password"></p>
    <p>비밀번호 확인:</br><input type="password" name="password_confirmation"></p>
    <input type="submit" value="입력">
</form>
